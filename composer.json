{
  "name": "codeinc/psr15-router-middleware",
  "version": "1.0.3",
  "description": "A PSR-15 router middleware",
  "homepage": "https://github.com/CodeIncHQ/Psr15RouterMiddleware",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.1",
    "psr/http-message": "^1.0",
    "psr/http-server-handler": "^1.0",
    "codeinc/psr7-responses": "^1.3",
    "psr/http-server-middleware": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7",
    "codeinc/middleware-dispatcher": "^1"
  },
  "autoload": {
    "psr-4": { "CodeInc\\Psr15RouterMiddleware\\": "src/" }
  },
  "autoload-dev": {
    "psr-4": { "CodeInc\\Psr15RouterMiddleware\\Tests\\": "tests/" }
  },
  "suggest": {
    "codeinc/middleware-dispatcher": "PSR-15 middleware dispatcher"
  },
  "authors": [
    {
      "name": "Joan Fabr√©gat",
      "email": "joan@codeinc.fr",
      "homepage": "https://www.codeinc.fr",
      "role": "developer"
    }
  ],
  "config": {
    "preferred-install": {
      "*": "dist"
    }
  }
}